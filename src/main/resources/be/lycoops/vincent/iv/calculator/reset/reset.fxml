<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="178.0" prefWidth="153.0" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.lycoops.vincent.iv.calculator.reset.ResetPresenter">
    <children>
        <Button mnemonicParsing="false" onAction="#reset" prefHeight="35.0" prefWidth="45.0" text="Reset" textAlignment="CENTER">
            <VBox.margin>
                <Insets top="45.0" />
            </VBox.margin>
        </Button>
        <Label text="Route">
            <VBox.margin>
                <Insets top="10.0" />
            </VBox.margin>
        </Label>
          <ComboBox fx:id="routeSelect" onAction="#reset" prefWidth="150.0">
             <VBox.margin>
                <Insets top="5.0" />
             </VBox.margin>
          </ComboBox>
      <CheckBox fx:id="askOnChangeButton" mnemonicParsing="false" onAction="#askOnChangeAction" text="Don't ask on reset">
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
      </CheckBox>
    </children>
    <padding>
        <Insets right="3.0" />
    </padding>
</VBox>
